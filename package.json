{
  "name": "cypress-test",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "delete:reports": "rm cypress/reports/* || true",
    "combine:reports": "jrm cypress/reports/combined-report.xml \"cypress/reports/*.xml\"",
    "pretest": "npm run delete:reports",
    "test": "npx cypress run -b chrome",
    "check-format": "prettier --check .",
    "format": "prettier --write .",
    "check-lint": "eslint . --ext ts",
    "check-all": "npm run check-format && npm run check-lint"
